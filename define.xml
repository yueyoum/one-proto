  <support>
    <defaulttype type="0" name="unknown" value="unknown"/>
    <defaulttype type="0100" name="byte" value="SByte"/>
    <defaulttype type="0100" name="char" value="SChar"/>
    <defaulttype type="0102" name="int" value="SInt"/>
