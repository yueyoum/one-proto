    <defaulttype type="0300" name="vector" value="List"/>
    <defaulttype type="0301" name="list" value="LinkedList"/>
    <defaulttype type="0302" name="set" value="HashSet"/>
    <defaulttype type="0400" name="map" value="Dictionary"/>
  </support>
  -->
  
